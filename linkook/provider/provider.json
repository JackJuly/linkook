    "HackerOne": {
        "mainUrl": "https://hackerone.com",
        "profileUrl": "https://hackerone.com/^USER^",
        "queryUrl": "https://hackerone.com/graphql",
        "keyword": {
            "notMatch": ["User does not exist"]
        },
        "request_method": "POST",
        "headers": {
        "Content-Type": "application/json"
        },
        "request_payload": {
        "operationName":"UserProfilePage","variables":{"resourceIdentifier":"^USER^"},"query":"query UserProfilePage($resourceIdentifier: String!) {\n\n  user(username: $resourceIdentifier) {\n    id\n    username\n    name\n    ...UserProfileCardUser\n  }\n}\n\n\n\nfragment UserProfileCardUser on User {\n  id\n  location\n  website\n  bio\n  bugcrowd_handle\n  hack_the_box_handle\n  github_handle\n  gitlab_handle\n  linkedin_handle\n  twitter_handle\n}\n\n\n\n"
        },
        "isConnected": true,
        "handle_regex" : {
            "Bugcrowd": "bugcrowd_handle\":\"([^\"]+)",
            "GitHub": "github_handle\":\"([^\"]+)",
            "GitLab": "gitlab_handle\":\"([^\"]+)",
            "LinkedIn": "linkedin_handle\":\"([^\"]+)",
            "X(Twitter)": "twitter_handle\":\"([^\"]+)",
            "Hack The Box": "hack_the_box_handle\":\"([^\"]+)"
        }
    },
    "Hack The Box": {
        "mainUrl": "https://app.hackthebox.com",
